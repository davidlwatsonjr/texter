{
  "name": "texter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "predeploy": "gcloud config set project davidlwatsonjr && gcloud -q artifacts docker images delete us-east1-docker.pkg.dev/davidlwatsonjr/cloud-run-source-deploy/texter --delete-tags",
    "deploy": "gcloud run deploy texter --source .",
    "postdeploy": "gsutil -m rm -r gs://davidlwatsonjr_cloudbuild/**",
    "docker:build": "docker build -t texter .",
    "predocker:start": "npm run docker:build",
    "docker:start": "docker run -p 8080:8080 texter",
    "eslint": "eslint",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "lint:fix": "eslint --fix src/**/*.js",
    "prettier": "prettier",
    "start": "node --env-file=.env src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@davidlwatsonjr/microservice-middleware": "^1.0.0",
    "express": "^4.18.2",
    "twilio": "^4.17.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5"
  }
}
